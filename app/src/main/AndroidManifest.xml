<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.techtibet.mathq">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".HighScoreActivity"
            android:theme="@style/CustomTheme"/>
        <activity
            android:name=".LevelOneActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".LevelTwoActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".LevelThreeActivity" />
        <activity android:name=".LevelFourActivity" />
        <activity android:name=".LevelFiveActivity" />
        <activity android:name=".LevelSixActivity" />
        <activity android:name=".LevelSevenActivity"></activity>
    </application>

</manifest>